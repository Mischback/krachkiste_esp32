{
  "C_Cpp.autoAddFileAssociations": false,
  "C_Cpp.intelliSenseEngine": "Tag Parser",
  "files.associations": {
    "sdkconfig": "kconfig",
    "sdkconfig.*": "kconfig"
  },
  "files.watcherExclude": {
    "${workspaceFolder}/.esp/**": true,
    "${workspaceFolder}/.tox/**": true,
    "${workspaceFolder}/.tox-env/**": true,
    "${workspaceFolder}/build/**": true
  }
}
